class Solution {
  public:
    const int MOD = 1e9 + 7; // So as to avoid int overflow
    vector<int> Series(int n) {
        // Code here
        if(n==0)return {0};
        if(n==1)return {0,1};
        vector<int>vec;
        vec.push_back(0);
        vec.push_back(1);
        for(int i=2;i<=n;i++){
            vec.push_back((vec[i-1]+vec[i-2]) % MOD); // At nth index, fibbonacci is equal to sum of n-1th term + n-2th term
        }
        return vec;
    }
};
