class Solution {
public:
    int maxSubArray(vector<int>& nums) { // Kadane's Algo
        int maxSum = INT_MIN;
        int sum = 0;
        for(auto x:nums){
            sum += x;
            maxSum = max(maxSum,sum);
            if(sum < 0){
                sum = 0;
            }
        }
        return maxSum;
    }
};
